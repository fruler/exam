  	<servlet-class>login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>show1</servlet-name>
  	<display-name>show1</display-name>
  	<description></description>
  	<servlet-class>show1</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>add</servlet-name>
  	<display-name>add</display-name>
  	<description></description>
  	<servlet-class>add</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>newshow</servlet-name>
  	<display-name>newshow</display-name>
