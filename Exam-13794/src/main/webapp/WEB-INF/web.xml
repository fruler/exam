  	<servlet-class>fdelete</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>fupdate1</servlet-name>
  	<display-name>fupdate1</display-name>
  	<description></description>
  	<servlet-class>fupdate1</servlet-class>
  </servlet>
  <servlet>
