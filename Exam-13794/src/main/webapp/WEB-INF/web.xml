<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<display-name>login</display-name>
  	<description></description>
  	<servlet-class>login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>show1</servlet-name>
  	<display-name>show1</display-name>
  	<description></description>
  	<servlet-class>show1</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>add</servlet-name>
  	<display-name>add</display-name>
  	<description></description>
  	<servlet-class>add</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>newshow</servlet-name>
  	<display-name>newshow</display-name>
  	<description></description>
  	<servlet-class>newshow</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>show1</servlet-name>
  	<url-pattern>/show1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>add</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>newshow</servlet-name>
  	<url-pattern>/newshow</url-pattern>
  </servlet-mapping>
</web-app>
