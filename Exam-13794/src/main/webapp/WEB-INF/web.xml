  	<servlet-class>fupdate2</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>show1</servlet-name>
  	<url-pattern>/show1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
